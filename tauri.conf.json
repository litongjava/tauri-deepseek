{
  "build": {
    "devPath": "https://chat.deepseek.com/",
    "distDir": "dist"
  },
  "package": {
    "productName": "tauri-deepseek",
    "version": "0.1.0"
  },
  "tauri": {
    "identifier": "com.litongjava.tauri.deepseek",
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "targets": "all"
    },
    "windows": [
      {
        "title": "Tauri deepseek",
        "fullscreen": false,
        "resizable": true,
        "height": 1000,
        "width": 1500,
        "url": "https://chat.deepseek.com/",
        "x": 100,
        "y": 0
      }
    ]
  }
}
